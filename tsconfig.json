{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["src/**/*.ts", "src/**/*.tsx", "worker-configuration.d.ts"],
	"exclude": ["temp", "node_modules"]
}
